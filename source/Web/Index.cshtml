@using System.Web.Optimization
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>Console</title>
    @Styles.Render("~/assets/css")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/xterm/3.14.5/xterm.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Droid+Sans:400,700|Droid+Sans+Mono|Droid+Serif" rel="stylesheet" type="text/css">
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
  </head>
  <body>
    <div id="welcome">
      <h1>Welcome to <u>watch.grossweber.com</u></h1>
      <p>You will see something here as soon as your presenter starts typing.</p>
      <p><strong>Please note:<br/></strong>This web page is stateless. All content will be gone if you refresh the page.
      </p>
    </div>

    <div id="status"></div>
    <div id="terminals"></div>

    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/signalr@2.4.1/jquery.signalR.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/xterm@4.5.0/lib/xterm.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/css-element-queries/1.2.3/ResizeSensor.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/xterm-addon-fit@0.3.0/lib/xterm-addon-fit.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/xterm-addon-web-links@0.3.0/lib/xterm-addon-web-links.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/fontfaceobserver@2.1.0/fontfaceobserver.standalone.min.js"></script>

    @Scripts.Render("~/assets/js")
  </body>
</html>
